{
  "GET /api/:date": {
    "description": "serves an intake of a given date",
    "requiredHeaders": {
      "Authorization": {
        "example": "Bearer theNewGeneratedToken.Comes.Here"
      }
    },
    "exampleRequestBody": {
        "userId": "7664566aa5e8aab81fb89b69"
    },
    "exampleResponse": {
      "intake": {
        "date": "2025-02-02",
        "kcal": 1001,
        "protein": 101,
        "carbs": 101
      }
    }
  },
  "POST /api/register": {
    "description": "serves an object reflecting the successful user register action",
    "exampleRequestBody": {
      "username": "Rita",
      "email": "rita@example.com",
      "password": "abshfh.6477HJ"
    },
    "exampleResponse": {
      "result": {
        "sucess": true
      }
    }
  },
  "POST /api/auth": {
    "description": "serves an object reflecting the generated token",
    "exampleRequestBody": {
      "email": "rita@example.com",
      "password": "abshfh.6477HJ"
    },
    "exampleResponse": {
      "token": "theTokenGenerated.Comes.Here"
    }
  },
  "POST /api/refresh-token": {
    "description": "serves an object reflecting the sucessful token generation operation and also the new generated token",
    "requiredHeaders": {
      "Authorization": {
        "example": "Bearer theNewGeneratedToken.Comes.Here"
      }
    },
    "exampleRequestBody": {
      "userId": "7664566aa5e8aab81fb89b69"
    },
    "exampleResponse": {
      "newToken": {
        "sucessNewToken": true,
        "token": "theNewGeneratedToken.Comes.Here"
      }
    }
  },
  "POST /api/add-intake": {
    "description": "serves an object reflecting the successful intake add operation and also the intake input",
    "requiredHeaders": {
      "Authorization": {
        "example": "Bearer theNewGeneratedToken.Comes.Here"
      }
    },
    "exampleRequestBody": {
      "userId": "7664566aa5e8aab81fb89b69",
      "date": "2025-02-02",
      "kcal": 1000,
      "protein": 100,
      "carbs": 100
    },
    "exampleResponse": {
      "result": {
        "sucess": true,
        "intake": {
          "date": "2025-02-02",
          "kcal": 1000,
          "protein": 100,
          "carbs": 100
        }
      }
    }
  },
  "PUT /api/add-more-intake": {
    "description": "serves an object reflecting the successful intake updating operation and also the updated intake",
    "requiredHeaders": {
      "Authorization": {
        "example": "Bearer theNewGeneratedToken.Comes.Here"
      }
    },
    "exampleRequestBody": {
      "userId": "7664566aa5e8aab81fb89b69",
      "date": "2025-02-02",
      "kcal": 1,
      "protein": 1,
      "carbs": 1
    },
    "exampleResponse": {
      "result": {
        "sucess": true,
        "updatedIntake": {
          "date": "2025-02-02",
          "kcal": 1001,
          "protein": 101,
          "carbs": 101
        }
      }
    }
  },
  "PUT /api/logout": {
    "description": "returns an object reflecting the successful logout operation",
    "exampleRequestBody": {
      "userId": "7664566aa5e8aab81fb89b69"
    },
    "exampleResponse": {
      "logoutSuccess": true
    }
  }
}
